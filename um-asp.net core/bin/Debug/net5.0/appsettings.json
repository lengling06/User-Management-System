{
  // 项目启动配置
  "webapi": {
    "Port": "9999"
  },
  // 配置Redis连接字符串
  "RedisOptions": {
    "ConnectionString": "localhost:6379"
  },
  "Logging": {
    // 日志配置
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  // 数据库连接字符串
  "ConnectionStrings": {
    "UmConnection": "Data Source=.;Initial Catalog=UMSystemDB;Integrated Security=True"
  }
}
